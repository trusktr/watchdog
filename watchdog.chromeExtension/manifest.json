{
	"name": "Watchdog",
	"version": "1.0",
	"manifest_version": 2,
	"description": "Controls and monitors the content player",
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},
	"permissions": [
		"tabs",
		"management",
		"http://*/*",
		"chrome-extension://*/*"
	],
	"background": {
		"page": "background.html"
	}
}